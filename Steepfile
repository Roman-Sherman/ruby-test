target :lib do
  signature "sig"
  check "lib"
  library "base64"
  library "cgi"
  library "forwardable"
  library "json"
  library "net-http"
  library "openssl"
  library "securerandom"
  library "tmpdir"
  library "uri"
  configure_code_diagnostics(Steep::Diagnostic::Ruby.default) # strict or all_error
end
